{
  "name": "greek-in-tech",
  "version": "0.1.0",
  "description": "An ES6, Backbone, single page application (SPA), that demonstrates many greek words and names that we constantly use on modern computing and software engineering.",
  "main": "index.html",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --devtool eval --progress --colors --hot --display-error-details",
    "build": "webpack -p",
    "test-webserver": "live-server --no-browser &",
    "test": "npm run test-unit && npm run test-functional",
    "test-unit": "mocha --compilers js:babel-register",
    "test-functional": "casperjs test tests-functional/simple.js",
    "lint": "eslint src/** test/**"
  },
  "dependencies": {
    "backbone": "^1.2.3",
    "hammerjs": "^2.0.6",
    "jquery": "^2.2.0",
    "requirejs": "^2.1.22",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "compass-mixins": "^0.12.7",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-defaults": "^9.0.0",
    "eslint-loader": "^1.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "live-server": "~0.8.1",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "raw-loader": "0.5.1",
    "requirejs-text": "~2.0.12",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.1"
  },
  "author": "John Tsevdos <tsevdosjohn@gmail.com>",
  "license": "MIT"
}
